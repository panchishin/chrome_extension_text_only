{
  "name": "Textonly",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extract paragraph text",
  "homepage_url": "",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "TEXT!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}

